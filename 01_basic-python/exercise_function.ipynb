    "def dist(p1, p2):\n",
    "    s = 0\n",
    "    for (x, y) in zip(p1, p2):\n",
    "        s += (x-y)**2\n",
    "    result = s ** 0.5\n",
    "    return result\n",
    "\n",
    "p = [2, 3, -1]\n",
    "q = [4, 1, -2]\n",
    "print(\"The result is\", dist(p, q))"
